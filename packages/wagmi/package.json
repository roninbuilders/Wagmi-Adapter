{
  "name": "@roninbuilders/wagmi-adapter",
  "version": "0.0.0-alpha.1",
  "description": "Wagmi connector for Ronin Wallet",
  "type": "module",
  "main": "./dist/esm/exports/config.js",
  "types": "./dist/types/exports/config.d.ts",
  "exports": {
    ".": {
      "types": "./dist/config.d.ts",
      "default": "./dist/config.js"
    },
    "./hooks": {
      "types": "./dist/hooks.d.ts",
      "default": "./dist/hooks.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm run clean && npx tsc",
    "watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roninbuilders/connectors.git"
  },
  "keywords": [
    "wagmi",
    "ronin",
    "wallet",
    "network",
    "blockchain",
    "connector"
  ],
  "author": "glitch-txs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roninbuilders/connectors/issues"
  },
  "homepage": "https://github.com/roninbuilders/connectors#readme",
  "dependencies": {
    "@walletconnect/ethereum-provider": "2.11.0",
    "wagmi": "2.5.4"
  },
  "devDependencies": {
    "@types/react": "18.2.48",
    "react": "18.2.0",
    "typescript": "5.3.3",
    "viem": "2.6.0"
  },
  "peerDependencies": {
    "react": ">=17",
    "typescript": ">=5",
    "viem": ">=2 <3"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
